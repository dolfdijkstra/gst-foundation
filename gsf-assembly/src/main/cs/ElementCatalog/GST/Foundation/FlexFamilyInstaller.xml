    <SETVAR NAME="item:sitessites" VALUE="Variables.empty" />
    <SETVAR NAME="item:assetsubtype" VALUE="AssetTypes.ITEM" />
    <SETVAR NAME="item:processinput" VALUE="notallowed" />
