    <SETVAR NAME="item:assettype" VALUE="AssetTypes.ITEM"/>
    <SETVAR NAME="item:itemtype" VALUE="ContentForm" />
    <SETVAR NAME="item:legalsitessites" VALUE="_ALL_" />
